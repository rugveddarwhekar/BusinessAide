<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.businessaide.businessaide">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/business_aide_first"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/business_aide_first_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EntryExitActivity" />
        <activity android:name=".ClickedActivity" />
        <activity android:name=".SendDataActivity" />
        <activity android:name=".ExitClickedActivity" />
        <activity android:name=".DataGen" />
        <activity android:name=".Signup"></activity>
    </application>

</manifest>